<h1>Listing partidos</h1>

<table>
  <tr>
    <th>Equipo1</th>
    <th>Equipo2</th>
    <th>Lugar</th>
    <th>Fecha</th>
    <th>Jugado</th>
    <th>Publicar</th>
  </tr>

<% @partidos.each do |partido| %>
  <tr>
    <td><%=h partido.equipo1 %></td>
    <td><%=h partido.equipo2 %></td>
    <td><%=h partido.lugar %></td>
    <td><%=h partido.fecha %></td>
    <td><%=h partido.jugado %></td>
    <td><%=h partido.publicar %></td>
    <td><%= link_to 'Show', partido %></td>
    <td><%= link_to 'Edit', edit_partido_path(partido) %></td>
    <td><%= link_to 'Destroy', partido, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New partido', new_partido_path %>